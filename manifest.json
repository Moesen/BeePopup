{
    "manifest_version": 2,
    "name": "SpellingBeeOverlay",
    "version": "1.1",

    "description": "Adds a popup that shows how many of each 2-word beginning is missing",
    "homepage_url": "https://github.com/moesen/BeePopup",


    "icons": {
        "48": "icons/48honeycomb.png",
        "96": "icons/96honeycomb.png"
    },

    "content_scripts": [
        {
            "matches": ["https://www.nytimes.com/puzzles/spelling-bee"],
            "js": ["overlay.js"]
        }
    ]
}

